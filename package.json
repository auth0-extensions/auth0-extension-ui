{
  "name": "auth0-extension-ui",
  "version": "0.3.0",
  "description": "Auth0 Extension UI",
  "engines": {
    "node": "6.0.0",
    "npm": "^3.0.0"
  },
  "main": "src/components/index.js",
  "scripts": {
    "release": "git tag $npm_package_version && git push && git push --tags && npm publish",
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner --require ignore-styles src/**/components/**/*.specs.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh",
    "postpublish": "npm run publish-storybook"
  },
  "keywords": [
    "auth0"
  ],
  "author": "auth0",
  "license": "MIT",
  "devDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "@kadira/storybook": "^2.2.1",
    "@kadira/storybook-addon-hello": "^1.0.1",
    "auth0-styleguide": "auth0/styleguide#4.6.5",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.19.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-auth0": "^6.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "file-loader": "^0.9.0",
    "git-url-parse": "^6.0.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^8.3.1",
    "mocha": "^2.4.5",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-codemirror": "^0.2.6",
    "react-dropzone": "^3.6.0",
    "react-loader-advanced": "^1.4.1",
    "react-redux": "^4.4.5",
    "react-select": "^1.0.0-rc.2",
    "redux-form": "^6.0.5",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url": "^0.11.0",
    "url-loader": "^0.5.7"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "redux": "^3.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.25.0",
    "path": "^0.12.7",
    "stylus-loader": "^2.3.1"
  }
}
